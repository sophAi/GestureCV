<?xml version="1.0" standalone="yes" encoding="UTF-8"?>
<VitrualInput>
	<name>
		Mouse click emulator
	</name>
	<description>
		Emulate mouse click and dragging
	</description>
	<detect_mode>
		mouse
	</detect_mode>
	<move_type>
		relative
	</move_type>
	<key1>
		-1 -1 -1 -1
	</key1>
	<key2>
		-1 -1 -1 -1
	</key2>
	<key3>
		-1 -1 -1 -1
	</key3>
	<button>
		-1 -1 -1 -1
	</button>
    <step>
         5  5  5  5
    </step>
    <move_factor>
         1  1  1  1
    </move_factor>
	<interval_us>
		15000 15000 15000 15000
	</interval_us>
</Vitrualinput>
<!-- Order: left right up down -->
<!-- detect_mode: key, mouse, and any (for using key and mouse at the same time, only effective on PC -->
<!-- move_type: absolute or relative -->
