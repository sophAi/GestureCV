<?xml version="1.0" standalone="yes" encoding="UTF-8"?>
<VitrualInput>
	<name>
		mplayer controler
	</name>
	<description>
		Control playback, pause, forward, and backward
	</description>
	<detect_mode>
		key
	</detect_mode>
	<move_type>
		absolute
	</move_type>
	<key1>
		105 106 57 57
	</key1>
	<key2>
		-1 -1 -1 -1
	</key2>
	<key3>
		-1 -1 -1 -1
	</key3>
	<button>
		-1 -1 -1 -1
	</button>
</Vitrualinput>
<!-- Order: left right up down -->
<!-- detect_mode: key, mouse, and any (for using key and mouse at the same time, only effective on PC -->
<!-- move_type: absolute or relative -->
